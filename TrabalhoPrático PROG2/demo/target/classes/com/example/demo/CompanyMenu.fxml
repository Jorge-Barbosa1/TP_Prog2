<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: steelblue;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.ControllerCompanyMenu">
   <children>
      <AnchorPane prefHeight="400.0" prefWidth="246.0" style="-fx-background-color: grey;">
         <children>
            <Label layoutX="44.0" layoutY="117.0" text="Menu Empresa">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <Button fx:id="logoutButton" layoutX="483.0" layoutY="361.0" mnemonicParsing="false" onAction="#logout" text="Terminar Sessão" />
      <Button fx:id="createEmployeeButton" layoutX="265.0" layoutY="146.0" mnemonicParsing="false" onAction="#addEmplyoee" prefHeight="25.0" prefWidth="85.0" text="Funcionário" />
      <Button fx:id="createServicesButton" layoutX="358.0" layoutY="146.0" mnemonicParsing="false" onAction="#addService" prefHeight="25.0" prefWidth="85.0" text="Serviços" />
      <Label layoutX="271.0" layoutY="245.0">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label layoutX="268.0" layoutY="112.0" text="Criar:">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label layoutX="266.0" layoutY="206.0" text="Listar:">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Button fx:id="listEmployeeButton" layoutX="266.0" layoutY="245.0" mnemonicParsing="false" onAction="#listEmployees" text="Funcionarios" />
      <Button fx:id="listServicesButton" layoutX="358.0" layoutY="245.0" mnemonicParsing="false" onAction="#listServices" prefHeight="25.0" prefWidth="85.0" text="Serviços" />
      <Button fx:id="listAppointmentsButton" layoutX="453.0" layoutY="245.0" mnemonicParsing="false" text="Marcações" />
      <Button fx:id="createLocationButton" layoutX="453.0" layoutY="146.0" mnemonicParsing="false" onAction="#addLocal" prefHeight="25.0" prefWidth="73.0" text="Local" />
   </children>
</AnchorPane>
