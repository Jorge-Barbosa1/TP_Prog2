<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: steelblue;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.ControllerListServices">
   <children>
      <AnchorPane prefHeight="92.0" prefWidth="600.0" style="-fx-background-color: grey;">
         <children>
            <Label layoutX="196.0" layoutY="27.0" text="Lista de Serviços">
               <font>
                  <Font name="System Bold" size="27.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <Button fx:id="goBackButton" layoutX="520.0" layoutY="361.0" mnemonicParsing="false" onAction="#goBack" text="Voltar" />
      <TableView fx:id="servicesView" layoutX="54.0" layoutY="110.0" prefHeight="219.0" prefWidth="491.0">
        <columns>
          <TableColumn fx:id="typeServiceColumn" prefWidth="174.0" text="Tipo de Serviço" />
           
          <TableColumn fx:id="priceColumn" minWidth="4.0" prefWidth="147.0" text="Preço" />
            <TableColumn fx:id="productColumn" minWidth="0.0" prefWidth="169.0" text="Produto Adicionado" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
