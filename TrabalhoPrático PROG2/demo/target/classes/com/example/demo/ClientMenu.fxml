<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: steelblue;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.ControllerClientMenu">
   <children>
      <AnchorPane prefHeight="400.0" prefWidth="239.0" style="-fx-background-color: grey;">
         <children>
            <Label layoutX="23.0" layoutY="136.0" text="Menu Cliente">
               <font>
                  <Font name="System Bold" size="31.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <Button fx:id="logoutButton" layoutX="486.0" layoutY="361.0" mnemonicParsing="false" onAction="#logout" text="Terminar Sessão" />
      <Button fx:id="createAppointmentButton" layoutX="329.0" layoutY="78.0" mnemonicParsing="false" onAction="#addAppointment" prefHeight="74.0" prefWidth="172.0" text="Fazer Marcação">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="paymentsButton" layoutX="329.0" layoutY="186.0" mnemonicParsing="false" prefHeight="74.0" prefWidth="172.0" text="Pagamentos">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
