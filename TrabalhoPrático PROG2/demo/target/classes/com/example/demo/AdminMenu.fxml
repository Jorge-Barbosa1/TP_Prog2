<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: steelblue;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.ControllerAdminMenu">
   <children>
      <Button fx:id="logoutButton" layoutX="483.0" layoutY="361.0" mnemonicParsing="false" onAction="#logout" text="Terminar Sessão" />
      <AnchorPane prefHeight="400.0" prefWidth="198.0" style="-fx-background-color: grey;">
         <children>
            <Label layoutX="18.0" layoutY="142.0" text="MENU ADMIN">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
            </Label>
            <Button fx:id="buttonRegisterAdmin" layoutX="48.0" layoutY="310.0" mnemonicParsing="false" onAction="#createAdmin" prefHeight="35.0" prefWidth="117.0" text="Criar Novo Admin" />
         </children>
      </AnchorPane>
      <Button fx:id="buttonListServices" layoutX="232.0" layoutY="171.0" mnemonicParsing="false" onAction="#listAllServices" prefHeight="35.0" prefWidth="123.0" text="Serviços" />
      <Label layoutX="335.0" layoutY="95.0" text="Consutar:">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Button fx:id="buttonListCompanys" layoutX="422.0" layoutY="171.0" mnemonicParsing="false" onAction="#listCompanies" prefHeight="35.0" prefWidth="123.0" text="Empresas" />
      <Button fx:id="buttonListPayments" layoutX="232.0" layoutY="231.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="123.0" text="Pagamentos" />
      <Button fx:id="buttonListAppointments" layoutX="425.0" layoutY="237.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="123.0" text="Marcações" />
   </children>
</AnchorPane>
